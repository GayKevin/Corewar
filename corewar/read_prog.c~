#include <fcntl.h>
#include <unistd.h>
#include <stdlib.h>
#include "corewar.h"
#include "op.h"

int	read_file_cor(char *av, int p, t_core *co, header_t *hd)
{
  int	fd;

  hd = malloc(sizeof(hd));
  fd = open(av, O_RDONLY);
  read(fd, hd, sizeof(hd));
  close(fd);
}
